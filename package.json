{
  "version": "0.0.0",
  "scripts": {
    "serve": "npm-run-all -p watch-tailwind serve-vuecli",
    "build": "npm-run-all -s build-tailwind build-vuecli",
    "lint": "vue-cli-service lint",
    "serve-vuecli": "vue-cli-service serve",
    "watch-tailwind": "tailwindcss-cli -i ./tailwind.css -o ./src/index.css --jit -w",
    "build-vuecli": "vue-cli-service build",
    "build-tailwind": "tailwindcss-cli -i ./tailwind.css -o ./src/index.css --jit --minify"
  },
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "core-js": "^3.6.5",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.7",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
